<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Button?>
<VBox prefHeight="307.0" prefWidth="100.0" xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.renanliberato.priceindex.search.SearchController">
    <children>
        <VBox prefHeight="50.0" prefWidth="90.0">
            <children>
                <Label text="From:" />
                <DatePicker fx:id="fromField" />
            </children></VBox>
        <VBox prefHeight="50.0" prefWidth="90.0">
            <children>
                <Label text="To:" />
                <DatePicker fx:id="toField" />
            </children></VBox>
        <VBox alignment="TOP_RIGHT" prefHeight="24.0" prefWidth="90.0">
            <children>
                <Button fx:id="searchButton" mnemonicParsing="false" text="Search" />
            </children></VBox>
    </children>
    <padding>
        <Insets left="5.0" right="5.0" top="25.0" />
    </padding>
</VBox>